# ========================================
# CONFIGURATION REACT APP - ADMIN FRONTEND
# ========================================

# Ne pas générer de source maps en production (optimisation)
GENERATE_SOURCEMAP=false

# Ignorer les vérifications de préinstallation
SKIP_PREFLIGHT_CHECK=true

# Port de l'application admin (différent du user frontend)
PORT=3001

# ========================================
# API BACKEND
# ========================================
# En développement local
REACT_APP_API_URL=http://localhost:5001

# En production (remplacer par l'URL réelle de votre API)
# REACT_APP_API_URL=https://api.votre-domaine.com
